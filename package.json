{
  "name": "beanstalk-plugin",
  "displayName": "Beanstalk",
  "description": "Language support and syntax highlighting for Beanstalk",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bs",
      "aliases": ["Beanstalk", "bs"],
      "extensions": ["bs"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/bs-logo.png",
        "dark": "./icons/bs-logo.png"
      }
    }],
    "grammars": [{
      "language": "bs",
      "scopeName": "source.bs",
      "path": "./syntaxes/bs.tmLanguage.json",
      "embeddedLanguages": {
        "meta.embedded.block.markdown": "markdown"
      }
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/H3licopter/beanstalk-plugin"
  }
}
